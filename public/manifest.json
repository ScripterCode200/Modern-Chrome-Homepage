{
    "manifest_version": 3,
    "name": "My Next.js Dashboard",
    "version": "1.0",
    "description": "A custom new tab dashboard built with Next.js",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "host_permissions": [
        "https://api.coingecko.com/*"
    ],
    "action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "storage"
    ]
}